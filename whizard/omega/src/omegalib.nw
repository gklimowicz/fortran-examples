  gl   = gv - ga
  e21  = t%e(2) + t%e(1)*(0,1)
  e21s = t%e(2) - t%e(1)*(0,1)
  b12  = t%b(1) + t%b(2)*(0,1)
  b12s = t%b(1) - t%b(2)*(0,1)
