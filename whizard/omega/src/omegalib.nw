function omega_check_helicity (m, smax, s) result (error)
  real(kind=default), intent(in) :: m
  integer, intent(in) :: smax, s
  logical :: error
  select case (smax)
  case (0)
     error = (s /= 0)
  case (1)
     error = (abs (s) /= 1)
  case (2)
     if (m == 0.0_default) then
        error = .not. (abs (s) == 1 .or. abs (s) == 4)
     else
        error = .not. (abs (s) <= 1 .or. abs (s) == 4)
     end if
  case (4)
     error = .true.
  case default
     error = .true.
  end select
