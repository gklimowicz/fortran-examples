  c_psir1%psi(1)%a(2) = (-kp) * psir%a(3) + ik2 * psir%a(4)
  c_psir1%psi(1)%a(3) = ik2 * psir%a(1) + (-kp) * psir%a(2)
  c_psir1%psi(1)%a(4) = (-km) * psir%a(1) + (-ik2) * psir%a(2)
