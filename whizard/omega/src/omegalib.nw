\subsection{Fermionic Vector and Axial Couplings}
There's more than one chiral representation.  This one is compatible
with HELAS~\cite{HELAS}.
\begin{equation}
  \gamma^0 = \begin{pmatrix} 0 & \mathbf{1} \\ \mathbf{1} & 0 \end{pmatrix},\;
  \gamma^i = \begin{pmatrix} 0 & \sigma^i \\ -\sigma^i & 0 \end{pmatrix},\;
  \gamma_5 = i\gamma^0\gamma^1\gamma^2\gamma^3
           = \begin{pmatrix} -\mathbf{1} & 0 \\ 0 & \mathbf{1} \end{pmatrix}
\end{equation}
Therefore
\begin{subequations}
\begin{align}
  g_S + g_P\gamma_5 &=
    \begin{pmatrix}
      g_S - g_P & 0         & 0         & 0         \\
      0         & g_S - g_P & 0         & 0         \\
      0         & 0         & g_S + g_P & 0         \\
      0         & 0         & 0         & g_S + g_P
    \end{pmatrix} \\
  g_V\gamma^0 - g_A\gamma^0\gamma_5 &=
    \begin{pmatrix}
      0         & 0         & g_V - g_A & 0         \\
      0         & 0         & 0         & g_V - g_A \\
      g_V + g_A & 0         & 0         & 0         \\
      0         & g_V + g_A & 0         & 0
    \end{pmatrix} \\
  g_V\gamma^1 - g_A\gamma^1\gamma_5 &=
    \begin{pmatrix}
      0           & 0           & 0         & g_V - g_A \\
      0           & 0           & g_V - g_A & 0         \\
      0           & - g_V - g_A & 0         & 0         \\
      - g_V - g_A & 0           & 0         & 0
    \end{pmatrix} \\
  g_V\gamma^2 - g_A\gamma^2\gamma_5 &=
    \begin{pmatrix}
      0               & 0              & 0              & -\ii(g_V - g_A) \\
      0               & 0              & \ii(g_V - g_A) & 0               \\
      0               & \ii(g_V + g_A) & 0              & 0               \\
      -\ii(g_V + g_A) & 0              & 0              & 0
    \end{pmatrix} \\
  g_V\gamma^3 - g_A\gamma^3\gamma_5 &=
    \begin{pmatrix}
      0           & 0         & g_V - g_A & 0           \\
      0           & 0         & 0         & - g_V + g_A \\
      - g_V - g_A & 0         & 0         & 0           \\
      0           & g_V + g_A & 0         & 0
    \end{pmatrix}
\end{align}
\end{subequations}
