{
  "name": "WHIZARD-GUI",
  "version": "0.0.1",
  "description": "A clean GUI to steer WHIZARD",
  "main": "./lib/server.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "prestart": "npm run build && npm run webpack",
    "start": "node lib/server.js",
    "eslint": "./node_modules/.bin/eslint src/*js",
    "watch": "./node_modules/.bin/babel src --out-dir lib -w & ./node_modules/.bin/nodemon & ./node_modules/.bin/webpack -w",
    "webpack": "node_modules/.bin/webpack",
    "test": "mocha && npm run eslint",
    "precoverage": "npm run build",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/_mocha -- 'test/*.js'"
  },
  "license": "GPL-2.0",
  "dependencies": {
    "body-parser": "^1.15.2",
    "ejs": "2.3.4",
    "es6-promise": "3.0.2",
    "es6-promisify": "3.0.0",
    "express": "4.12.4",
    "jquery": "2.2.3",
    "js-logger": "1.2.0",
    "logging": "2.0.21",
    "mongodb": "2.0.33"
  },
  "devDependencies": {
    "babel-runtime": "6.9.2",
    "babel-cli": "6.7.5",
    "babel-core": "^6.1.2",
    "babel-eslint": "6.0.2",
    "babel-loader": "^6.0.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "^6.1.2",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "chai-jquery": "2.0.0",
    "chai-as-promised": "5.3.0",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-plugin-import": "1.4.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "8.4.0",
    "jsdom-global": "1.7.0",
    "mocha": "2.4.5",
    "mocha-jsdom": "^1.1.0",
    "nodemon": "*",
    "autoprefixer-loader": "^2.0.0",
    "css-loader": "^0.15.1",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "node-libs-browser": "^0.5.2",
    "style-loader": "^0.12.3",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.12.1"
  }
}
