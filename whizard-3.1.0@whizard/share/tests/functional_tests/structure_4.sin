# SINDARIN input for WHIZARD self-test

model = "QED"
model = "QCD"
model = "SM"
?logging = true
int i = 0
scan i = (1, 3) { show (i) }
scan i = () { show (i) }
scan int j = (2, 4) { show (j) }
scan int j = () { show (j) }
scan real x = (2.14)
scan string $s = ("foo", "bar", "gee") { show ($s) }
scan logical ?f = (true, false)
scan alias qq = (u:d, u:d:s:c)
scan cuts = (true, false) { show (cuts) }
scan weight = (1, 2, 3) { show (weight) }
scan scale = (100) { show (scale) }
scan renormalization_scale = (120) { show (renormalization_scale) }
scan factorization_scale = (240) { show (factorization_scale) }
scan selection = (true) { show (selection) }
scan reweight = (0.9) { show (reweight) }
scan analysis = (true) { show (analysis) }
scan model = ("QCD", QED) { show ($model_name)}
library = "structure_4_lib1"
scan library = ("structure_4_lib1")

