   rsb_coo_idx_t    IA[] = { 0, 0, 0, 0 };
        rsb_coo_idx_t    JA[] = { 0, 0, 0, 0 };
        RSB_DEFAULT_TYPE VA[] = { -1, -1, -1, -1 };
        rsb_trans_t transA = RSB_TRANSPOSITION_N;
        const rsb_coo_idx_t frA = 2, lrA = 2;
        rsb_nnz_idx_t rnz;
        RSB_DEFAULT_TYPE *alphap = NULL;

